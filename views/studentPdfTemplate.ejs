        font-style: italic;
        }

        .roboto-bold-italic {
        font-family: "Roboto", sans-serif;
        font-weight: 700;
        font-style: italic;
        }

        .roboto-black-italic {
        font-family: "Roboto", sans-serif;
        font-weight: 900;
        font-style: italic;
        }

    </style>
</head>
<body>
    <!-- <div class="header-wrapper">
        <img src="http://localhost:3025/header-new.png" alt="Header Image">
    </div> -->
    <div class="container">
        <div class="sub-c">
            <section class="basic-information">
                <img src="http://localhost:3025/comp-logo.png" alt="Company logo">
                <img src="https://iili.io/2NGXsMF.png" style="width: 350px; height: 50px;">
                <div class="heading-select">
                    <h1><%= student.reportsData.reportNameHeader %></h1>
                    <h2><%= student.reportsData.reportNameSubHeader %></h2>
                    <h3><%= student.studentData.fullName %></h3>
                </div>
                <!-- <div class="company-logo">
                    <img src="http://localhost:3025/college-logo.png" alt="Company logo">
                </div>    -->
            </section>
            <section class="profile">
                <!-- <img src="http://localhost:3025/logo.png" alt="Student Image"> -->
                 <div class="profile-image"></div>
                <div class="info">
                    <div class="info-detail">
                        <span><strong>Name:</strong></span> 
                        <span><%= student.studentData.fullName %></span>
                    </div>
                    <div class="info-detail">
                        <span><strong>Class:</strong></span> 
                        <span><%= student.studentData.standard %></span>
                    </div>
                    <div class="info-detail">
                        <span><strong>Roll No:</strong></span> 
                        <span><%= student.studentData.rollNo %></span>
                    </div>
                    <div class="info-detail">
                        <span><strong>Exam Date:</strong></span> 
                        <span><%= student.examData.examDate %></span>
                    </div>
                    <div class="info-detail">
                        <span><strong>School Name:</strong></span> 
                        <span><%= student.studentData.schoolName %></span>
                    </div>
                </div>
            </section>
    
        </div>
       
        
        <section class="performance-summary">
            <h2>Overall Performance Summary</h2>
            <!-- <div class="cool-graphic">
                <div>
                    <h3>75%</h3>
                </div>
            </div> -->
        </section>
        <div class="sub-c">
            <div class="remarks">
                <p><%= student.reportsData.performanceHighlights.summary %></p>
            </div>
    
            <div class="total-marks">
                <div>
                    <div class="total-marks-detail">
                        <span><strong>Total Marks Obtained:</strong></span> 
                        <span><%= student.examData.marksObtained %> out of <%= student.examData.totalExamMarks %></span>
                    </div>
                    <div class="total-marks-detail">
                        <span><strong>Result Status:</strong></span> 
                        <span><%= student.examData.passingStatus %></span>
                    </div>
                    <div class="total-marks-detail">
                        <span><strong>Remarks:</strong></span> 
                        <span><%= student.examData.remarks %></span>
                    </div>
                </div>
                <div class="circle-container">
                    <div class="circle percentage-<%= student.examData.percentage %>">
                        <span><%= student.examData.percentage %>%</span>
                        <div class="percentage-bar"></div>
                    </div>
                </div>
            </div>
        </div>
        

        
        <div class="sub-c">
            <table>
                <thead>
                    <tr>
                        <th>Sections</th>
                        <th>Marks Obtained</th>
                        <th>Maximum Marks</th>
                        <th>Performance Level</th>
                    </tr>
                </thead>
                <tbody>
                    <% student.examData.sectionData.forEach(function(section) { %>
                    <tr>
                        <td><%= section.name %></td>
                        <td><%= section.marks %></td>
                        <td><%= section.totalMarks %></td>
                        <td><%= section.performanceLevel %></td>
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>

        <section class="performance-performane-summary">
            <h2>PERFORMANCE HIGHLIGHTS</h2>
        </section>
        <div class="performance-info">
            <% student.reportsData.performanceHighlights.sectionData.forEach(function(sData) { %>
            <div>
                <span><strong><%= sData.title %>:</strong></span> 
                <span><%= sData.description %></span>
            </div>
            <% }) %>       
        </div>
        <div id="container" style="width: 80%; height: 280px; margin-left: 100px;"></div>

        <section class="performance-strength-summary">
            <h2>STRENGTHS</h2>
        </section>
        <div class="performance-info">
            <div>
                <span><%= student.reportsData.strengths.description %></span>
            </div>    
        </div>
        <section class="performance-summary">
            <h2>AREAS FOR IMPROVEMENT</h2>
        </section>
        <div class="performance-info">
            <div>
                <span><%= student.reportsData.areasOfImprovement.description %></span>
            </div>    
        </div>
        <section class="performance-summary">
            <h2>RECOMMEDNED CAREER</h2>
        </section>
        <div class="performance-info">
            <div>
                <span><%= student.reportsData.recommendedCareer.description %></span>
            </div>    
        </div>
        <section class="performance-summary">
            <h2>CONCLUSION</h2>
        </section>
        <div class="performance-info">
            <div>
                <span><%= student.reportsData.conclusion.description %></span>
            </div>    
        </div>
        <section class="thank-you">
            <div>
                <img src="http://localhost:3025/thankyou-02.png" alt="Thank you" style="width: 100%; height: 850px;">
                <!-- <img src="http://localhost:3025/udayami.png" alt="Udayami" style="width: 100%; margin-top: 20px;"> -->
            </div>
        </section>
        <!-- <div>
            <img src="http://localhost:3025/thankyou.png" alt="Thank you" style="width: 100%; height: 80%;">
            <img src="http://localhost:3025/udayami.png" alt="Udayami">
        </div> -->
    </div>
    <!-- <script>
        // Set current date in the footer dynamically
        const currentDate = new Date().toLocaleDateString();
        document.getElementById('currentDate').textContent = currentDate;
    </script> -->
    <script>
        const sectionData = <%- JSON.stringify(student.examData.sectionData) %>;
        const categories = sectionData.map(section => section.name);
        const scores = sectionData.map(section => section.marks);
        const performanceLevels = sectionData.map(section => section.performanceLevel);

        const subjectWiseTotalMarks = sectionData.map(section => section.totalMarks);
        const maxNumber = Math.max(...subjectWiseTotalMarks);
        // console.log("subjectWiseTotalMarks::", maxNumber);

        // const totalExamMarks = <%- JSON.stringify(student.examData.totalExamMarks) %>;
        // const totalSubject = <%- JSON.stringify(student.examData.sectionData.length) %>;
        // const perSubjectTotalMarks = Math.ceil(totalExamMarks / totalSubject);
        // Create dynamic tick positions based on totalExamMarks
        const tickStep = Math.ceil(maxNumber / 5);  // Step size for ticks (you can adjust this logic)
        const tickPositions = [];
        for (let i = 0; i <= maxNumber; i += tickStep) {
            tickPositions.push(i);
        }

        const colorMapping = {
            "Excellent": "#24b200",    // Green for Excellent
            "Good": "#ffe678", // Yellow for Good
            "Average": "#fc9803", // Orange for Good
            "Poor": "#e74c3c"     // Red for Poor
        };

        const colors = performanceLevels.map(level => colorMapping[level]);
        console.log(colors);
        document.addEventListener('DOMContentLoaded', function() {
            Highcharts.chart('container', {
                chart: {
                    type: 'bar'
                },
                title: {
                    text: 'Student Performance'
                },
                xAxis: {
                    categories: categories,
                    title: {
                        text: ''
                    },
                    style: {
                        fontWeight: 'bold'
                    },
                    labels: {
                        style: {
                            fontWeight: 'bold'
                        }
                    }
                },
                yAxis: {
                    title: {
                        text: ''
                    },
                    tickPositions: tickPositions,
                    labels: {
                        style: {
                            fontWeight: 'bold'
                        }
                    }
                },
                series: [{
                    name: 'Score',
                    data: scores,
                    colorByPoint: true,
                    colors: colors
                }],
                credits: {
                    enabled: false
                },
                legend: {
                    enabled: false
                }
            });
        });

      </script>
</body>
</html>
